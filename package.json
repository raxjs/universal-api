{
  "name": "@uni/apis",
  "private": true,
  "version": "1.0.9",
  "scripts": {
    "setup": "rm -rf node_modules && yarn install --no-lockfile",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx ./src/packages/*/src --fix",
    "build:demo": "cross-env BUILD_TYPE=all node ./scripts/buildDemo.js",
    "build": "cross-env node ./scripts/build.js",
    "build:all": "cross-env BUILD_TYPE=all node ./scripts/build.js",
    "build:main": "cross-env BUILD_TYPE=main node ./scripts/build.js",
    "pub": "cross-env node ./scripts/publish.js",
    "pub:all": "cross-env BUILD_TYPE=all node ./scripts/publish.js",
    "pub:main": "cross-env BUILD_TYPE=main node ./scripts/publish.js",
    "pub-beta": "cross-env BUILD_TAG=beta node ./scripts/publish.js",
    "pub-beta:all": "cross-env BUILD_TYPE=all BUILD_TAG=beta node ./scripts/publish.js",
    "pub-beta:main": "cross-env BUILD_TYPE=main BUILD_TAG=beta node ./scripts/publish.js",
    "docs:dev": "cross-env node ./scripts/buildDocs.js && cross-env dumi dev",
    "docs:build": "cross-env node ./scripts/buildDocs.js && cross-env dumi build",
    "docs:deploy": "npm run docs:build && gh-pages -ad docs-dist",
    "check-and-publish": "node ./scripts/check-and-publish.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.5",
    "style-unit": "^3.0.0",
    "weex-bindingx": "^0.0.49"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.6.3",
    "@commitlint/cli": "^12.0.0",
    "@iceworks/spec": "^1.1.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "axios": ">=0.21.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "build-scripts": "^0.1.25",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.1.0",
    "commitizen": "^4.2.3",
    "console-clear": "^1.1.1",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^1.1.0-beta.30",
    "eslint": "^6.8.0",
    "fs-extra": "^9.0.1",
    "gh-pages": "^3.1.0",
    "glob": "^7.1.4",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-replace": "^1.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.5",
    "koa-compose": "^4.1.0",
    "minimist": "^1.2.0",
    "ora": "^5.1.0",
    "parallelshell": "^3.0.2",
    "prettier": "^2.2.1",
    "rax-scripts": "^2.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-visualizer": "^4.1.2",
    "semver": "^6.3.0",
    "shelljs": "^0.8.4",
    "typescript": "^3.9.6",
    "webpack-node-externals": "^2.5.2"
  }
}
